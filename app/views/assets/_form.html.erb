<!-- TODO convert form to semantic form and fix form paragraphing and styling-->
<%= form_for @asset do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :uploaded_file, "File" %><br />
    <%= f.file_field :uploaded_file %>
    <%= f.hidden_field :folder_id %>
  </p>
  <p><%= f.submit "Upload"%></p>
<% end %>
